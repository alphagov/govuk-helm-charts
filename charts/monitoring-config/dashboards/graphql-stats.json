            "includeByName": {},
            "indexByName": {
              "Time 1": 5,
              "Time 2": 6,
              "Time 3": 7,
              "Time 4": 8,
              "Value #A": 2,
              "Value #B": 1,
              "Value #C": 3,
              "Value #D": 4,
              "document_type": 0
            },
            "renameByName": {
              "Time 1": "",
              "Time 2": "",
              "Value #A": "GraphQL time",
              "Value #B": "Content Store time",
              "Value #C": "GraphQL Requests",
              "Value #D": "Content Store Requests"
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "mode": "reduceRow",
            "reduce": {
              "include": [
